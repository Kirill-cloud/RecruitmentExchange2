<UserControl x:Class="RecruitmentExchange.View.Applicant.RemoveApplicant"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RecruitmentExchange.View.Applicant"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center"  VerticalAlignment="Center">
            <Label FontSize="26" Content="{Binding TabName}"></Label>
            <Button Command="{Binding Remove}" >Продолжить</Button>
        </StackPanel>
    </Grid>
</UserControl>
